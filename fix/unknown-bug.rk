rk:start
   int #int
   key #key sub

   print: "Enter a character and I'll convert it to an index: "

   read: #key

   int #int = #key

   if #int >= 'a' && #int <= 'z' then
      print: "Lowercase letter!^n"
      key sub = 'a'
   fi

   if #int >= '0' && #int <= '9' then
      print: "Number!^n"
      key sub = '0'
   fi

   if #int >= 'A' && #int <= 'Z' then
      print: "Uppercase letter!^n"
      key sub = 'Z'
   fi

   #int -= sub

   print: #int
rk:end
